{
  "version": 2,
  "builds": [
    {
      "src": "Backend/Server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/Server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 