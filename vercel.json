{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "env": {
    "MONGO_URI": "@mongo_uri",
    "JWT_SECRET": "@jwt_secret"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/Server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}